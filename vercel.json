{
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/leaderboard", "destination": "/leaderboard.html" },
    { "source": "/mazing", "destination": "/mazing.html" },
    { "source": "/tutorials", "destination": "/tutorials.html" },
    { "source": "/ranks", "destination": "/ranks.html" },
    { "source": "/profile", "destination": "/profile.html" }
  ],
  "headers": [
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*).js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*).css",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, must-revalidate"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ]
}